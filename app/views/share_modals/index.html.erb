

<h1>Listing Share Modals</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @share_modals.each do |share_modal| %>
      <tr>
        <td><%= share_modal.title %></td>
        <td><%= share_modal.content %></td>
        <td><%= share_modal.slug %></td>
        <td><%= link_to 'Show', share_modal %></td>
        <td><%= link_to 'Edit', edit_share_modal_path(share_modal) %></td>
        <td><%= link_to 'Destroy', share_modal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Share modal', new_share_modal_path %>
